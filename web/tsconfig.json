{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@types": ["src/types"],
      "@lib/*": ["src/lib/*"],
      "@utils/*": ["src/utils/*"],
      "@styles/*": ["src/styles/*"],
      "@public/*": ["public/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
